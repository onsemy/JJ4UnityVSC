{
  "name": "jj4unity",
  "displayName": "JJ4Unity",
  "description": "This is a VSCode extension to facilitate the use of Unity within Visual Studio Code.",
  "version": "0.0.1",
  "publisher": "onsemy",
  "engines": {
    "vscode": "^1.94.0"
  },
  "homepage": "https://github.com/onsemy/JJ4UnityVSC",
  "repository": {
    "type": "git",
    "url": "https://github.com/onsemy/JJ4UnityVSC.git"
  },
  "categories": [
    "Other",
    "Extension Packs"
  ],
  "bugs": "https://github.com/onsemy/JJ4UnityVSC/issues",
  "keywords": [
    "unity",
    "unity3d",
    "jj4unity"
  ],
  "activationEvents": [
    "onLanguage:csharp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
    ]
